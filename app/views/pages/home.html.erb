<h1>Welcome</h1>
<p>Find me in app/views/pages/home.html.erb</p<table>
<h2>Cats</h2>
<table>
  <tr>
    <td>name</td>
  </tr>
  <% @cats.each do |cat| %>
    <tr>
      <td><%= cat.name%></td>
    </tr>
  <% end %>
</table>
<h3>Users</h3>
<table>
  <tr>
    <td>username</td>
    <td>email</td>
    <td>age</td>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username%></td>
      <td><%= user.email%></td>
      <td><%= user.age%></td>
    </tr>
  <% end %>
</table>
<h3>Todos</h3>
<table>
  <tr>
    <td>task</td>
    <td>finished</td>
  </tr>
  <% @todos.each do |todo| %>
    <tr>
      <td><%= todo.task%></td>
      <td><%= todo.finished%></td>
    </tr>
  <% end %>
</table>